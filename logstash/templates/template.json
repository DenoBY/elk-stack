{
  "index_patterns": ["*-nginx-*", "*-laravel-*", "*-php-fpm-*"],
  "priority": 200,
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 1
    },
    "mappings": {
      "properties": {
        "message": {
          "type": "text",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 8192
            }
          }
        }
      }
    }
  }
}
